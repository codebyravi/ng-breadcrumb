{
  "name": "ng-breadcrumb",
  "version": "1.0.6",
  "license": "MIT",
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  },
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/breadcrumb.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/breadcrumb.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify",
    "release": "ng-packagr -p package.json"
  },
  "main": "dist/index.js",
  "repository": "git@github.com:Catalysts/ng-breadcrumb.git",
  "devDependencies": {
    "@angular/cli": "6.0.7",
    "@angular/common": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~4.3.0",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "10.1.1",
    "protractor": "5.3.2",
    "rollup": "^0.55.3",
    "rxjs": "^6.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.10.0",
    "typescript": "2.7.2",
    "uglify-js": "^3.3.10",
    "zone.js": "^0.8.26"
  }
}
